{
  "manifest_version": 2,
  "name": "LinkedIn Job Filter",
  "version": "1.0",
  "description": "Filter and highlight relevant job listings on LinkedIn",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["jquery-3.7.1.min.js", "jquery.highlight.js", "content.js"]
    }
  ]
}